{
  "name": "imaginary-health",
  "version": "1.0.0",
  "workspaces": [
    "frontend",
    "studio"
  ],
  "scripts": {
    "build": "npm run build:studio && npm run build:frontend",
    "build:studio": "cd studio && npm run build",
    "build:frontend": "cd studio && npm run build",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:studio\"",
    "dev:frontend": "npm --workspace frontend run dev",
    "dev:studio": "npm --workspace studio run dev",
    "dev:imaginary": "SITE_ID=imaginary-health concurrently \"npm run dev:frontend\" \"npm run dev:studio\"",
    "dev:thems": "SITE_ID=thems concurrently \"npm run dev:frontend\" \"npm run dev:studio\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
